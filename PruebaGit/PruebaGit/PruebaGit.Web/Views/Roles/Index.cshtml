@model IEnumerable<PruebaGit.Web.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
}

<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-10">
            <div class="card">
                <div class="card-header bg-info">
                    <h2 class="text-center">Lista de Usuarios</h2>
                </div>
                <div class="card-body">
                    <table class="table table-hover table-sm">
                        <tr>
                            <td>Item</td>
                            <td>UserName</td>
                            <td>Email</td>
                            <td>Add Rol</td>
                        </tr>
                        @{
                            int count = 1;
                        }

                        @* ERROR EN LA LINEA 24 en el foreach *@
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@count</td>
                                <td>@item.UserName</td>
                                <td>@item.Email</td>
                                <td><a href="@Url.Action("AddRoles","Roles",new { id = item.Id })"class="btn btn-primary">Add</a></td>
                            </tr>
                            count++;
                        }
                        @* ERROR EN LA LINEA 24 en el foreach *@

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

